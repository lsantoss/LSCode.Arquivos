<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>LSCode</Authors>
    <Company>LSCode</Company>
    <Copyright>LSCode</Copyright>
    <AssemblyVersion>2022.02.09.1</AssemblyVersion>
    <Description>Oferece códigos para facilitar a manipulação de arquivos nos projetos produzidos pela LSCode</Description>
    <Product>LSCode.Files</Product>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
